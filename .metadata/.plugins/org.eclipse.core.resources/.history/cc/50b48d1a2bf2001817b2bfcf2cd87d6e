package {
	import laya.webgl.WebGL;
	
	/**根*/
	public class Root {
		private var myInt: int = 0;
		private var myArr: Array = [1, 2, 3, 4];
		public function Root() {
			//初始化引擎
			Laya.init(750, 1334,WebGL);
			
			var a: int = this.myArr[0];
			var b: int = this.myArr[1];
			var c: int = a + b;
			console.log('a = ' + a);
			console.log('b = ' + b);
			console.log('c = ' + c);
		}
	}
}