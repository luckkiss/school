package view
{
	import ui.PlayerUI;
	
	public class PlayerView extends PlayerUI
	{
		public function PlayerView()
		{
			super();
		}
		
		public function setName(value: String): void {
			this.textName.text = value;
		}
		
		public function setScore(value: int): void {
			this.textScore.text = '得分：' + value;
		}
	}
}